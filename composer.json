    "authors": [
        {
            "name": "nishizuka",
            "email": "nishizuka@spookies.co.jp"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
       	"test": "phpunit"
    },
    "autoload": {                                                                                                                                          
        "psr-4": {
            "Narnia\\": "src/Narnia/"
        }
    },  
