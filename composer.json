    "keywords": ["php","laravel"],
    "require": {
        "laravel/framework": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "psy/psysh": "@stable",
        "orchestra/testbench": "^3.8"
